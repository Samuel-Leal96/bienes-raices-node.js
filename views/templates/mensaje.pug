extends ../layout/index

block contenido
    div.py10
        h1(class = "text-4xl my-10 font-extrabold text-center") Bienes
            span(class = "font-normal") Raices
        h2.text-center.text-2xl.font-extrabold= pagina

        p(class="text-xl font-bold text-center my-10")= mensaje

        if error
            button(
                type="submit" 
                class="btn w-full bg-indigo-600 hover:bg-indigo-800 text-white font-bold py-2 cursor-pointer rounded-lg text-center" 
                onclick="window.location.href='/auth/registro'"
            ) Regresar